{
  "name": "sol-scaffold",
  "version": "0.1.0",
  "description": "A CLI to scaffold common DeFi primitives on Solana",
  "main": "dist/index.js",
  "bin": {
    "sol-scaffold": "dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gemini/sol-scaffold.git"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "defi",
    "scaffold",
    "cli"
  ],
  "author": "Gemini",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "fs-extra": "^11.3.2",
    "inquirer": "^9.3.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/rimraf": "^3.0.2",
    "jest": "^30.2.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}